{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "settings": {
    "import/extensions": [".js"],
    "import/resolver": {
      "node": {
        "alwaysTryTypes": true
      },
      "babel-module": {}
    }
  },
  "rules": {
    "import/no-unresolved": "off",
    "no-console": "off",
    "import/extensions": "off"
  }
}
